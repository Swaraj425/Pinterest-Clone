<% include ./partials/header.ejs %>

    <div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
        <div class="cards flex flex-wrap gap-10">
            <% user.posts.forEach(function(elem){ %>
                <div class="card w-40">
                    <div class="w-full h-40 bg-sky-300 rounded-lg">
                        <img class="w-full h-full object-cover rounded-lg" src="/images/uploads/<%= elem.image %>" alt="">
                    </div>
                    <h4 class="mt-2 text-sm"><%= elem.title %></h4>
                </div>
            <% }) %>
        </div>
    </div>

<% include ./partials/footer.ejs %>